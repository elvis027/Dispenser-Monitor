<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

    <TextView
            android:text="lat_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/lat_textview"
            android:textSize="20sp"
            app:layout_constraintTop_toTopOf="@+id/lat"
            app:layout_constraintStart_toEndOf="@+id/lat"
            android:layout_marginStart="8dp"
            android:layout_marginLeft="8dp"
            app:layout_constraintBottom_toBottomOf="@+id/lat"/>
    <TextView
            android:text="lng_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/lng_textview"
            android:textSize="20sp"
            app:layout_constraintTop_toTopOf="@+id/lng"
            app:layout_constraintStart_toEndOf="@+id/lng"
            android:layout_marginStart="8dp"
            android:layout_marginLeft="8dp"
            app:layout_constraintBottom_toBottomOf="@+id/lng"/>
    <TextView
            android:text="created_at_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/created_at_textview"
            android:textSize="20sp"
            app:layout_constraintTop_toTopOf="@+id/created_at"
            app:layout_constraintStart_toEndOf="@+id/created_at"
            android:layout_marginStart="8dp"
            android:layout_marginLeft="8dp"
            app:layout_constraintBottom_toBottomOf="@+id/created_at"/>
    <TextView
            android:text="updated_at_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/updated_at_textview"
            android:textSize="20sp"
            app:layout_constraintTop_toTopOf="@+id/updated_at"
            app:layout_constraintStart_toEndOf="@+id/updated_at"
            android:layout_marginStart="8dp"
            android:layout_marginLeft="8dp"
            app:layout_constraintBottom_toBottomOf="@+id/updated_at"/>
    <TextView
            android:text="macaddr_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/macaddr_textview"
            android:textSize="20sp"
            app:layout_constraintStart_toEndOf="@+id/macaddr"
            android:layout_marginLeft="8dp"
            android:layout_marginStart="8dp"
            app:layout_constraintTop_toTopOf="@+id/macaddr"
            app:layout_constraintBottom_toBottomOf="@+id/macaddr"/>
    <TextView
            android:text="data_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/data_textview"
            android:textSize="20sp"
            app:layout_constraintStart_toEndOf="@+id/data"
            android:layout_marginStart="8dp"
            android:layout_marginLeft="8dp"
            app:layout_constraintBottom_toBottomOf="@+id/data"
            app:layout_constraintTop_toTopOf="@+id/data"/>
    <TextView
            android:text="macaddr:"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/macaddr"
            android:textSize="20sp"
            android:textStyle="bold"
            android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/id"
            app:layout_constraintStart_toStartOf="@+id/id"/>
    <TextView
            android:text="lat:"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/lat"
            android:textSize="20sp"
            android:textStyle="bold"
            android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/data"
            app:layout_constraintStart_toStartOf="@+id/data"/>
    <TextView
            android:text="data:"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/data"
            android:textSize="20sp"
            android:textStyle="bold"
            android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/macaddr"
            app:layout_constraintStart_toStartOf="@+id/macaddr"/>
    <TextView
            android:text="lng:"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/lng"
            android:textSize="20sp"
            android:textStyle="bold"
            app:layout_constraintStart_toStartOf="@+id/lat"
            android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/lat"/>
    <TextView
            android:text="created_at:"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/created_at"
            android:textSize="20sp"
            android:textStyle="bold"
            android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/lng"
            app:layout_constraintStart_toStartOf="@+id/lng"/>
    <TextView
            android:text="updated_at:"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/updated_at"
            android:textSize="20sp"
            android:textStyle="bold"
            android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/created_at"
            app:layout_constraintStart_toStartOf="@+id/created_at"
            android:layout_marginBottom="16dp"
            app:layout_constraintBottom_toBottomOf="parent"/>
    <TextView
            android:text="id:"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/id"
            android:textSize="20sp"
            android:textStyle="bold"
            android:layout_marginTop="16dp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="16dp"
            android:layout_marginStart="16dp"/>
    <TextView
            android:text="id_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/id_textview"
            android:textSize="20sp"
            app:layout_constraintStart_toEndOf="@+id/id"
            android:layout_marginLeft="8dp"
            android:layout_marginStart="8dp"
            app:layout_constraintTop_toTopOf="@+id/id"
            app:layout_constraintBottom_toBottomOf="@+id/id"/>

</android.support.constraint.ConstraintLayout>